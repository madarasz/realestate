    $(function () {
        var timeSinceLoadStarted = function () {
            return parseFloat(((new Date().getTime() / 1000) - window.loadStartedAt).toFixed(2));
        }

        if (window.loadStartedAt) {
            window.domLoadTime = timeSinceLoadStarted();
            $(window).load(function () {
                window.windowLoadTime = timeSinceLoadStarted();
                if (window.logToBackend) {
                    var msg = 'dom.ready: ' + window.domLoadTime + 's window.load: ' + window.windowLoadTime + 's';
                    var page = ' page: ' + $('body').attr('id');
                    window.logToBackend('DOM_LOAD_TIME', msg + page);
                }
            });
        }
    });
