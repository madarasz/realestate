    (function (d, it) {
        if (!it.version) {
            window.icomtracker = it;

            it.sourceAppName = '';
            it.init = function (options) {
                options = options || {};

                if (options.sourceAppName) {
                    it.sourceAppName = options.sourceAppName;
                }

                if (options.collectorUrl) {
                    it.collectorUrl = options.collectorUrl;
                }

                it.data = it.data || [];

                function m(it, f) {
                    it[f] = function () {
                        it.data.push([f].concat(Array.prototype.slice.call(arguments, 0)))
                    }
                }

                var f = ['track'];
                for (g = 0; g < f.length; g++) {
                    m(it, f[g]);
                }
            }
        }

        var h = d.getElementsByTagName('head')[0];
        var s = d.createElement('script');
        s.type = 'text/javascript';
        s.async = 1;
        s.src = '/js/tracker/icom-event-tracker-1.0.js';
        h.appendChild(s);
    })(document, window.icomtracker || []);

